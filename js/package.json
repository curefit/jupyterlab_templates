{
  "name": "jupyterlab_templates",
  "version": "0.3.2",
  "description": "Notebook templates",
  "author": "the jupyterlab_templates authors",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpmorganchase/jupyterlab_templates.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "scripts": {
    "build": "babel src/ --source-maps --out-dir lib/",
    "build:lab": "rimraf ../jupyterlab_templates/labextension && jupyter labextension build .",
    "build:all": "npm run build && npm run build:lab",
    "clean": "rimraf lib",
    "fix": "yarn lint --fix",
    "lint": "eslint -c .eslintrc.js --ext .js src/ tests/",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage --collectCoverageFrom=src/*.{js}"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.css"
  ],
  "jupyterlab": {
    "extension": "lib/index.js",
    "outputDir": "../jupyterlab_templates/labextension",
    "discovery": {
      "server": {
        "base": {
          "name": "jupyterlab_templates"
        },
        "managers": [
          "pip"
        ]
      }
    }
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.0",
    "@jupyterlab/apputils": "^3.1.0",
    "@jupyterlab/coreutils": "^5.1.0",
    "@jupyterlab/filebrowser": "^3.1.0",
    "@jupyterlab/launcher": "^3.1.0",
    "@jupyterlab/mainmenu": "^3.1.0",
    "@jupyterlab/notebook": "^3.1.0",
    "@lumino/disposable": "^1.7.0",
    "marked": "^4.0.10",
    "node-notifier": "^10.0.0",
    "requests-helper": "^0.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.9",
    "@jupyterlab/builder": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.0.6",
    "jest-transform-css": "^2.0.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2"
  },
  "resolutions": {
    "marked": "^2.0.0",
    "node-notifier": "^8.0.1"
  }
}
